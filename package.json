{
  "name": "addressr",
  "version": "1.0.0",
  "description": "Free Australian Address Validation, Search and Autocomplete ",
  "author": "Mountain Pass <addressr@mountain-pass.com.au>",
  "engines": {
    "node": ">=10.16.0"
  },
  "config": {
    "localport": "8080"
  },
  "main": "index.js",
  "scripts": {
    "requirements-check": "./scripts/check-version.js",
    "postinstall": "npm run requirements-check",
    "start": "DEBUG=error,api,xplanBody,express:*,swagger-tools* PORT=$npm_package_config_localport babel-node index.js",
    "watch:start": "nodemon -V  --ext \"*.js, *.json, *.yaml\" -x npm run start",
    "npm-check": "npm-check  --skip-unused",
    "npm-check:interactive": "npm-check --skip-unused -u ",
    "npm-check-unused": "npm-check",
    "lint": "node_modules/.bin/eslint controllers/*.js --fix"
  },
  "keywords": [
    "swagger",
    "g-naf",
    "address"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "dependencies": {
    "connect": "^3.2.0",
    "debug": "^4.1.1",
    "js-yaml": "^3.3.0",
    "swagger-tools": "^0.10.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^9.2.0",
    "ngrok": "^3.2.2",
    "nodemon": "^1.19.1",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "prettier-config-standard": "^1.0.1",
    "semver": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mountain-pass/addressr.mountain-pass.com.au.git"
  },
  "bugs": {
    "url": "https://github.com/mountain-pass/addressr.mountain-pass.com.au/issues"
  },
  "homepage": "https://addressr.mountain-pass.com.au",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
