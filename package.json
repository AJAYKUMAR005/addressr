{
  "name": "addressr",
  "version": "1.0.0",
  "description": "Free Australian Address Validation, Search and Autocomplete ",
  "author": "Mountain Pass <addressr@mountain-pass.com.au>",
  "engines": {
    "node": ">=11.14.0"
  },
  "config": {
    "localport": "8080"
  },
  "main": "server.js",
  "scripts": {
    "requirements-check": "./scripts/check-version.js",
    "postinstall": "npm run requirements-check",
    "start:loader:OT": "MONGO_COLLECTION_NAME=ot ES_INDEX_NAME=ot COVERED_STATES=OT DEBUG=error,api,express:*,swagger-tools*,test,es,mongo PORT=$npm_package_config_localport babel-node loader.js",
    "start:loader": "DEBUG=error,api,express:*,swagger-tools*,test,es,mongo PORT=$npm_package_config_localport babel-node loader.js",
    "start:server": "DEBUG=error,api,express:*,swagger-tools*,test,es,mongo PORT=$npm_package_config_localport babel-node server.js",
    "watch:start:server": "nodemon -V  --ext \"*.js, *.json, *.yaml\" -x npm run start:server",
    "npm-check": "npm-check  --skip-unused",
    "npm-check:interactive": "npm-check --skip-unused -u ",
    "npm-check-unused": "npm-check",
    "lint": "node_modules/.bin/eslint controllers/*.js --fix",
    "test": "NO_STRICT=' ' npm-run-all --serial test:component test:system",
    "watch:test": "nodemon -V  --ext \"*.feature, *.js, *.json, *.css, *.yaml\" -x npm -- run test",
    "test:component": "MONGO_COLLECTION_NAME=test ES_INDEX_NAME=test COVERED_STATES=OT DEBUG=error,api,express:*,swagger-tools*,test,es TEST_PROFILE=default cucumber-js -p default -- --harmony_async_iteration",
    "watch:test:component": "nodemon -V  --ext \"*.feature, *.js, *.json, *.css, *.yaml\" -x npm -- run test:component",
    "test:system": "MONGO_COLLECTION_NAME=test ES_INDEX_NAME=test COVERED_STATES=OT DEBUG=error,api,express:*,swagger-tools*,test,es,mongo TEST_PROFILE=system cucumber-js -p system -- --harmony_async_iteration",
    "watch:test:system": "nodemon -V  --ext \"*.feature, *.js, *.json, *.css, *.yaml\" -x npm -- run test:system",
    "test:component:QLD": "MONGO_COLLECTION_NAME=test ES_INDEX_NAME=test COVERED_STATES=QLD DEBUG=error,api,express:*,swagger-tools*,test,es,mongo TEST_PROFILE=default cucumber-js -p default -- --harmony_async_iteration"
  },
  "keywords": [
    "swagger",
    "g-naf",
    "address"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "dependencies": {
    "auth0": "^2.18.0",
    "cfonts": "^2.4.4",
    "connect": "^3.2.0",
    "debug": "^4.1.1",
    "directory-exists": "^2.0.1",
    "dotenv": "^8.0.0",
    "elasticsearch": "^16.3.0",
    "etag": "^1.8.1",
    "js-yaml": "^3.3.0",
    "json-ptr": "^1.1.2",
    "keyv": "^3.1.0",
    "keyv-file": "^0.1.13",
    "mongodb": "^3.3.0-beta2",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.0.0",
    "swagger-tools": "^0.10.4",
    "unzip-stream": "^0.3.0",
    "uri-template-lite": "^19.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@windyroad/cucumber-js-throwables": "^1.0.4",
    "@windyroad/quick-containers-js": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "dockerode": "^2.5.8",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "got": "^9.6.0",
    "http-link-header": "^1.0.2",
    "husky": "^3.0.0",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^9.2.0",
    "ngrok": "^3.2.2",
    "nodemon": "^1.19.1",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "prettier-config-standard": "^1.0.1",
    "semver": "^6.2.0",
    "wait-port": "^0.2.2",
    "why-is-node-running": "^2.1.0",
    "wtfnode": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mountain-pass/addressr.mountain-pass.com.au.git"
  },
  "bugs": {
    "url": "https://github.com/mountain-pass/addressr.mountain-pass.com.au/issues"
  },
  "homepage": "https://addressr.mountain-pass.com.au",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
