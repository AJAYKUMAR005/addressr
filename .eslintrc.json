{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "prettier", "prettier/babel"],
  "env": {
    "browser": false,
    "node": true
  },
  "globals": {
    "document": true,
    "window": true,
    "Image": true,
    "localStorage": true,
    "fetch": true,
    "exports": true,
    "process": true,
    "console": true,
    "__dirname": true,
    "global": true,
    "expect": true,
    "require": true,
    "module": true,
    "URL": true,
    "URLSearchParams": true,
    "setTimeout": true,
    "setInterval": true,
    "Promise": true
  },
  "rules": {
    "quotes": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "strict": 2,
    "prettier/prettier": "error",
    "import/default": 0
  },
  "plugins": ["prettier"]
}
